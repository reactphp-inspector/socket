{
  "name": "react-inspector/socket",
  "description": "\ud83d\uddc4\ufe0f Open Telemetry react/socket auto instrumentation",
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "ext-opentelemetry": "^1.1.3",
    "evenement/evenement": "^3.0",
    "open-telemetry/api": "^1.4.0",
    "open-telemetry/context": "^1.2.1",
    "open-telemetry/sdk": "^1.6",
    "open-telemetry/sem-conv": "^1.32.1",
    "react/socket": "^1.0.0",
    "react/stream": "^1.4"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^10.0.0",
    "wyrihaximus/makefiles": "^0.7.5"
  },
  "autoload": {
    "psr-4": {
      "ReactInspector\\Socket\\": "src"
    },
    "files": [
      "_register.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ReactInspector\\Tests\\Socket\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupol/composer-packages": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "mindplay/composer-locator": true,
      "php-http/discovery": false,
      "phpstan/extension-installer": true,
      "tbachert/spi": false,
      "wyrihaximus/broadcast": true,
      "wyrihaximus/makefiles": true,
      "wyrihaximus/test-utilities": true
    },
    "platform": {
      "php": "8.4.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "make on-install-or-update || true"
    ],
    "post-update-cmd": [
      "make on-install-or-update || true"
    ]
  }
}
